    "class Solution:\n",
    "    # @param n, an integer\n",
    "    # @return an integer\n",
    "    def reverseBits(self, n):\n",
    "        ans=0\n",
    "        for i in range(31,-1,-1):\n",
    "            ans=(ans<<1)|(n&1)\n",
    "            n>>=1\n",
    "        return ans"
   ]
