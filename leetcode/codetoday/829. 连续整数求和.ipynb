    "class Solution:\n",
    "    def consecutiveNumbersSum(self, n):\n",
    "        ans=0\n",
    "        n*=2\n",
    "        first=1\n",
    "        while first*first<n:\n",
    "            if n%first==0 and (n//first+1-first)%2==0:\n",
    "                ans+=1\n",
    "            first+=1\n",
    "        return ans\n"
   ]
  }
